{
    "name": "webmozart/json",
    "description": "A robust JSON decoder/encoder with support for schema validation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "justinrainbow/json-schema": ">=1.3.7",
        "seld/jsonlint": "~1.0"
    },
    "require-dev": {
      "symfony/serializer": "~2.0|~3.0.0"
    },
    "suggest": {
      "symfony/serializer": "To integrate with the Symfony Serializer."
    },
    "autoload": {
        "psr-4": {
            "Webmozart\\Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmozart\\Json\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
